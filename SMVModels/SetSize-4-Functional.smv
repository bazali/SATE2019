			--LTLSPEC NAME Fun_6_6 := G((inp=size -> out=0) -> X(inp=add1 -> X(inp=add2 -> X(inp=remov1 -> X(inp=remov2 -> X(inp=size -> out=0))))))												--True				
		
		
