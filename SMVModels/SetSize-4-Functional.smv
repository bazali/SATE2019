			LTLSPEC NAME Fun_3_8 := G((inp=size & out=0) -> X(inp=add1 -> X(inp=add2 -> X(inp=size -> out=2))))																--True
			
			--LTLSPEC NAME Fun_3_9 := G((inp=size & out=1) -> X(inp=add1 -> X(inp=add2 -> X(inp=size -> out=3))))															--True	
			
