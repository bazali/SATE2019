			--LTLSPEC NAME Fun_6_4 := G((inp=size & out=2) -> X(inp=add1 -> X(!FullSet & inp=add2 -> X(inp=remov1 -> X(inp=remov1 -> X(inp=size -> out=2))))))							--True for Stack/Queue not for Set
	
